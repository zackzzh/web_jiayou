.cases_head_div {
  width: 100%;
  min-width: 1200px;
}
.cases_head_div .cases_head_img {
  width: 100%;
  height: auto;
}
.cases_head_div .cases_head_title {
  width: 1200px;
  margin: 40px auto;
  padding-left: 20px;
  font-size: 22px;
  color: #999B9B;
}
.cases_head_div .cases_head_title .cases_head_title_span {
  text-decoration: none;
  cursor: pointer;
  color: #999B9B;
}
.new_info_main_div {
  position: relative;
  width: 1200px;
  margin: auto;
  padding: 0 0 50px 0;
}
.new_info_main_div .new_info_left_div {
  display: inline-block;
  width: 66%;
}
.new_info_main_div .new_info_left_div .new_info_title {
  color: #333333;
  font-family: SourceHanSansCN-Bold;
  font-size: 40px;
  font-weight: bold;
  text-align: center;
}
.new_info_main_div .new_info_left_div .new_info_subtitle {
  text-align: center;
}
.new_info_main_div .new_info_left_div .new_info_subtitle .new_info_time {
  display: inline-block;
  margin: 10px 20px 40px 20px;
  color: #333333;
  font-family: SourceHanSansCN-Regular;
  font-size: 24px;
  font-weight: 400;
}
.new_info_main_div .new_info_left_div .new_info_bottom_others {
  margin: 20px 0;
}
.new_info_main_div .new_info_left_div .new_info_bottom_others a {
  display: block;
  text-decoration: none;
  cursor: pointer;
  margin: 6px 0;
  font-size: 16px;
}
.new_info_main_div .new_info_back_top {
  display: none;
  cursor: pointer;
  position: fixed;
  right: 200px;
  bottom: 100px;
}
.new_info_main_div .new_info_back_top .new_info_back_top_img {
  width: 66px;
}
.new_info_main_div .news_right_main_div {
  display: inline-block;
  width: 33%;
  padding-left: 40px;
  vertical-align: top;
}
.new_info_main_div .news_right_main_div .news_right_div {
  background: #FFFFFF;
  box-shadow: 0px 8px 40px 0px rgba(0, 0, 0, 0.21);
  padding: 0 0 30px 0;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div {
  padding: 30px 0 0 0;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div a {
  text-decoration: none;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_title {
  padding: 0 40px;
  font-family: SourceHanSansCN-Regular;
  font-size: 22px;
  font-weight: 400;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_line {
  margin: 0 0 10px 0;
  padding: 0 40px;
  width: 90%;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_line .news_right_card_line_img {
  width: 100%;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_item_div {
  cursor: pointer;
  padding: 10px 40px;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_item_div .news_right_card_item_img_div {
  display: inline-block;
  width: 29%;
  vertical-align: middle;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_item_div .news_right_card_item_img_div .news_right_card_item_img {
  width: 100%;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_item_div .news_right_card_item_content_div {
  display: inline-block;
  width: 69%;
  vertical-align: middle;
  padding-left: 14px;
  color: #3F3F3F;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_item_div .news_right_card_item_content_div a {
  text-decoration: none;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_item_div .news_right_card_item_content_div .news_right_card_item_title {
  margin-bottom: 4px;
  font-family: SourceHanSansCN-Medium;
  font-size: 15px;
  font-weight: 600;
  color: #3F3F3F;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_item_div .news_right_card_item_content_div .news_right_card_item_content {
  font-family: SourceHanSansCN-Regular;
  font-size: 13px;
  font-weight: 400;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_ranking_list .news_right_card_ranking_item {
  display: block;
  cursor: pointer;
  text-decoration: none;
  padding: 8px 50px;
  font-family: SourceHanSansCN-Regular;
  font-size: 16px;
  font-weight: 400;
  color: #3F3F3F;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_ranking_list .news_right_card_ranking_item:hover {
  background: #F2F2F2;
}
.new_info_main_div .news_right_main_div .news_right_div .news_right_card_div .news_right_card_item_div:hover {
  background: #F2F2F2;
}
