.none_jy_head {
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 10;
}

.none_main_div {
    width: 100%;
    height: calc(100vh);
    position: relative;
    text-align: center;

    .none_img {
        width: 100%;
        height: 100%;
    }

    .none_div {
        position: absolute;
        bottom: 100px;
        right: 0;
        left: 0;
        margin: auto;

        .none_title {
            font-family: SourceHanSansCN-Regular;
            font-weight: 400;
            font-size: 30px;
            text-shadow: 0px 0px 10px rgba(0, 0, 0, 1);
            color: #FFFFFF;
        }

        .none_btn {
            margin-top: 40px;
            padding: 16px 40px;
            border-radius: 31px;
            font-size: 18px;
            font-family: OpenSans-SemiBold;
            font-weight: 600;
            background: #FFFFFF;
        }
    }
}