.index_main_div {
  min-width: 1200px;
}
.index_main_div .carousel-control {
  opacity: 0.6;
}
.index_main_div .carousel-control .glyphicon {
  font-size: 46px;
}
.index_main_div .carousel-control.right {
  background-image: none;
}
.index_main_div .carousel-control.left {
  background-image: none;
}
.index_main_div .carousel_img {
  width: 100%;
  height: auto;
}
.index_main_div .index_content_main_div {
  width: 1200px;
  text-align: center;
  margin: auto;
  padding: 46px 0 70px 0;
}
.index_main_div .index_content_main_div .index_hot_div {
  margin: 60px 0 70px 0;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card {
  position: relative;
  width: 228px;
  height: 320px;
  display: inline-block;
  margin: 0 4px;
  transition: height 1s;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_bg_div {
  height: 320px;
  border-radius: 6px;
  background: #EBEBEB;
  box-shadow: 0px 2px 9px 0px rgba(0, 27, 176, 0.27);
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_bg_div .index_hot_bg_top {
  height: 198px;
  border-radius: 6px 6px 0 0;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_bg_div .index_hot_bg_top_color01 {
  background: #97F1FF;
  box-shadow: 0px 0px 133px 0px rgba(0, 90, 228, 0.39) inset;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_bg_div .index_hot_bg_top_color02 {
  background: #7BAAF3;
  box-shadow: 0px 0px 117px 0px rgba(0, 9, 124, 0.48) inset;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_bg_div .index_hot_bg_top_color03 {
  background: #E0E8DA;
  box-shadow: 0px 0px 169px 0px rgba(92, 211, 255, 0.48) inset;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_bg_div .index_hot_bg_top_color04 {
  background: #B47BD5;
  box-shadow: 0px 0px 101px 0px rgba(16, 52, 194, 0.63) inset;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_bg_div .index_hot_bg_top_color05 {
  background: #E392C5;
  box-shadow: 0px 10px 122px 0px rgba(0, 61, 215, 0.4) inset;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_content {
  text-align: center;
  position: absolute;
  bottom: 12px;
  left: 0;
  right: 0;
  margin: auto;
  letter-spacing: 4px;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_content .index_hot_card_img {
  animation: hot_card_img 1400ms linear infinite alternate;
  -webkit-animation: hot_card_img 1400ms linear infinite alternate;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_content .index_hot_card_img01 {
  width: 100%;
  height: 230px;
  padding: 0 8px 0 16px;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_content .index_hot_card_img02 {
  width: 102%;
  margin: -220px 0 0 -22px;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_content .index_hot_card_img03 {
  width: 104%;
  margin: 10px -30px 0 0;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_content .index_hot_card_img04 {
  width: 102%;
  padding: 0 24px 14px 2px;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_content .index_hot_card_img05 {
  width: 100%;
  height: 230px;
  padding: 0 20px 0px 24px;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_content .index_hot_card_title {
  margin-top: 4px;
  font-size: 36px;
  font-weight: 500;
  color: #383838;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_card_modal {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 0;
  border-radius: 6px;
  padding: 0 20px;
  font-family: SourceHanSansCN-Medium;
  font-weight: 600;
  letter-spacing: 4px;
  overflow: hidden;
  transition: height 1s;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_card_modal .index_hot_card_modal_title {
  margin: 30px 0 16px 0;
  font-size: 32px;
  color: #006E95;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_card_modal .index_hot_card_modal_content {
  text-align: left;
  font-size: 14px;
  color: #000000;
  opacity: 0.7;
  line-height: 22px;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_card_modal01 {
  background: #97F1FF;
  box-shadow: 0px 0px 133px 0px rgba(0, 90, 228, 0.39) inset;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_card_modal02 {
  background: #7BAAF3;
  box-shadow: 0px 0px 117px 0px rgba(0, 9, 124, 0.48) inset;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_card_modal03 {
  background: #E0E8DA;
  box-shadow: 0px 0px 169px 0px rgba(92, 211, 255, 0.48) inset;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_card_modal04 {
  background: #B47BD5;
  box-shadow: 0px 0px 101px 0px rgba(16, 52, 194, 0.63) inset;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card .index_hot_card_modal05 {
  background: #E392C5;
  box-shadow: 0px 10px 122px 0px rgba(0, 61, 215, 0.4) inset;
}
.index_main_div .index_content_main_div .index_hot_div .index_hot_card:hover .index_hot_card_modal {
  height: 100%;
  display: block;
}
.index_main_div .index_content_main_div .index_cases_main_div {
  margin: 0 0 50px 0;
  padding: 30px 0 0 0;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_options_div {
  margin: 20px 0 0 0;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_options_div a {
  text-decoration: none;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_options_div .index_cases_option {
  cursor: pointer;
  display: inline-block;
  margin: 0 30px;
  font-family: SourceHanSansCN-Medium;
  font-size: 24px;
  font-weight: 500;
  color: #2B85EA;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_options_div .index_cases_option_active {
  color: #333333;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_options_div .index_cases_option_more {
  cursor: pointer;
  float: right;
  padding: 6px 14px;
  border-radius: 6px;
  background: #7C90CF;
  color: #FFFFFF;
  font-size: 16px;
  font-weight: 400;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div {
  width: 100%;
  margin-top: 40px;
  text-align: left;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div {
  position: relative;
  display: inline-block;
  width: 33%;
  padding: 8px;
  transition: height 1s;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div .index_case_img_div {
  width: 100%;
  border-radius: 4px;
  box-shadow: 0px 6px 16px 0px rgba(0, 0, 0, 0.6);
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div .index_case_img_div .index_case_img {
  width: 100%;
  height: auto;
  border-radius: 4px;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div .index_case_modal_div {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 0;
  text-align: center;
  vertical-align: middle;
  padding: 8px;
  transition: height 1s;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div .index_case_modal_div .index_case_modal {
  padding: 0 8px;
  height: 100%;
  vertical-align: middle;
  color: #FEFEFE;
  background: #333333;
  font-family: MicrosoftYaHei-Bold;
  font-weight: bold;
  letter-spacing: 2px;
  border-radius: 4px;
  overflow: hidden;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div .index_case_modal_div .index_case_modal .index_case_modal_title {
  font-size: 18px;
  margin: 12px 0 6px 0;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div .index_case_modal_div .index_case_modal .index_case_modal_content {
  margin-bottom: 6px;
  font-size: 12px;
  text-indent: 26px;
  text-align: left;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div .index_case_modal_div .index_case_modal .index_case_modal_line {
  cursor: pointer;
  display: inline-block;
  padding: 4px 20px;
  border: 1px solid #2CFDB4;
  border-radius: 6px;
  font-size: 14px;
  color: #2CFDB4;
  background: transparent;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div .index_case_modal_div .index_case_modal .index_case_qr_main_div {
  height: 50px;
  text-align: center;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div .index_case_modal_div .index_case_modal .index_case_qr_main_div .index_case_qr_div {
  display: inline-block;
  width: 50px;
  height: 100%;
  margin: 0 20px;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div .index_case_modal_div .index_case_modal .index_case_qr_main_div .index_case_qr_div img {
  display: inline-block;
  width: 100%;
  height: 100%;
}
.index_main_div .index_content_main_div .index_cases_main_div .index_cases_div .index_case_div:hover .index_case_modal_div {
  height: 100%;
}
.index_main_div .index_content_main_div .index_news_main_div {
  margin: 50px 0 0 0;
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div {
  width: 49%;
  display: inline-block;
  vertical-align: top;
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div .index_news_div {
  width: 100%;
  position: relative;
  padding: 24px 30px;
  border-radius: 6px;
  background: #ffffff;
  box-shadow: 0px 0px 9px 0px rgba(0, 0, 0, 0.25);
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div .index_news_div .index_news_main_title {
  width: 100%;
  margin-bottom: 20px;
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div .index_news_div .index_news_main_title .index_news_title {
  width: 49%;
  text-align: left;
  display: inline-block;
  font-family: SourceHanSansCN-Bold;
  font-size: 28px;
  font-weight: bold;
  color: #333333;
  letter-spacing: 6px;
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div .index_news_div .index_news_main_title .index_news_more {
  text-decoration: none;
  cursor: pointer;
  width: 49%;
  display: inline-block;
  text-align: right;
  font-family: SourceHanSansCN-Regular;
  font-size: 20px;
  font-weight: 400;
  color: #333333;
  letter-spacing: 4px;
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div .index_news_div .index_new_item {
  cursor: pointer;
  width: 100%;
  margin: 16px 0 0 0;
  font-family: SourceHanSansCN-Regular;
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div .index_news_div .index_new_item a {
  text-decoration: none;
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div .index_news_div .index_new_item .index_new_item_title {
  width: 77%;
  display: inline-block;
  text-align: left;
  font-size: 18px;
  font-weight: 600;
  color: #2253BD;
  letter-spacing: 2px;
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div .index_news_div .index_new_item .index_new_item_time {
  width: 22%;
  display: inline-block;
  text-align: right;
  font-size: 16px;
  color: #7C90CF;
  letter-spacing: 2px;
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div .index_news_div .index_new_item:hover .index_new_item_title {
  color: #76B0FF;
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div_odd {
  padding: 0 10px 0 0;
}
.index_main_div .index_content_main_div .index_news_main_div .index_news_div_div_even {
  padding: 0 0 0 10px;
}
@keyframes hot_card_img {
  0% {
    margin-bottom: 0;
  }
  100% {
    margin-bottom: 10px;
  }
}
@-webkit-keyframes hot_card_img {
  0% {
    margin-bottom: 0;
  }
  100% {
    margin-bottom: 10px;
  }
}
